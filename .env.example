APP_NAME=CredentialVault
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8000

# Database Configuration
# Use MySQL for compatibility with XAMPP
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=password
DB_USERNAME=root
DB_PASSWORD=

# Cache & Session (Laravel 11 uses CACHE_STORE instead of CACHE_DRIVER)
CACHE_STORE=file
SESSION_DRIVER=file
QUEUE_CONNECTION=sync

# Frontend URL for OAuth redirect
FRONTEND_URL=http://localhost:3000

# Google OAuth Credentials
# Get these from: https://console.cloud.google.com/
# IMPORTANT: Use the SAME credentials as Go backend for consistency
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:8000/auth/google/callback

# Encryption Key (optional - Laravel uses APP_KEY for encryption)
# This is kept for compatibility with Go backend documentation
ENCRYPTION_KEY=